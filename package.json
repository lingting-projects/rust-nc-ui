{
  "name": "ant-design-pro",
  "version": "6.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "npm run build:dev",
    "build:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider relative=true max build",
    "build:relative": "cross-env NODE_OPTIONS=--openssl-legacy-provider relative=true max build",
    "build:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider REACT_APP_ENV=prod relative=true UMI_ENV=prod max build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "npm run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider PORT=30000 REACT_APP_ENV=dev UMI_ENV=dev max dev",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie > 0"
  ],
  "dependencies": {
    "@ant-design/icons": "~5.0.0",
    "@ant-design/pro-components": "~2.8.2",
    "@monaco-editor/react": "~4.6.0",
    "@umijs/route-utils": "~4.0.1",
    "antd": "~5.22.3",
    "antd-style": "~3.7.1",
    "classnames": "~2.5.1",
    "crypto-js": "~4.2.0",
    "lodash": "~4.17.21",
    "moment": "~2.30.1",
    "omit.js": "~2.0.2",
    "querystring": "~0.2.1",
    "rc-menu": "~9.16.0",
    "rc-util": "~5.43.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-helmet-async": "~1.3.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "~3.3.0",
    "@testing-library/react": "~13.4.0",
    "@types/classnames": "~2.3.1",
    "@types/crypto-js": "~4.2.0",
    "@types/express": "~4.17.21",
    "@types/history": "~4.7.11",
    "@types/lodash": "~4.14.202",
    "@types/react": "~18.2.48",
    "@types/react-dom": "~18.2.18",
    "@types/react-helmet": "~6.1.11",
    "@umijs/fabric": "~2.14.1",
    "@umijs/lint": "~4.3.31",
    "@umijs/max": "~4.3.31",
    "@umijs/plugins": "~4.3.31",
    "cross-env": "~7.0.3",
    "eslint": "~8.56.0",
    "express": "~4.18.2",
    "gh-pages": "~3.2.3",
    "lint-staged": "~10.5.4",
    "monaco-editor": "~0.52.2",
    "prettier": "~2.8.8",
    "react-dev-inspector": "~1.9.0",
    "ts-node": "~10.9.2",
    "typescript": "~5.3.3",
    "umi-presets-pro": "~2.0.3",
    "umi-serve": "~1.9.11"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
